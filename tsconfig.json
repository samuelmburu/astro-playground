{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@scripts/*": [
        "scripts/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}